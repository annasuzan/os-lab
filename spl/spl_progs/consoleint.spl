[PROCESS_TABLE + [SYSTEM_STATUS_TABLE+1]*16 + 13] = SP; // UPTR
SP = [PROCESS_TABLE + [SYSTEM_STATUS_TABLE+1]*16 + 11]*512 - 1; // Kernel Stack
backup;

alias PID R10;

[TERMINAL_STATUS_TABLE+0] = 0;
PID = [TERMINAL_STATUS_TABLE+1];
[PROCESS_TABLE+ PID*16 + 8] = P0;

R1 = RELEASE_TERMINAL;
R2 = PID;
call RESOURCE_MANAGER;

restore;

SP = [PROCESS_TABLE + [SYSTEM_STATUS_TABLE+1]*16 + 13]; // UPTR
ireturn;
